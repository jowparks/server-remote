    "@tamagui/lucide-icons": "^1.91.3",
    "@tamagui/menu": "^1.95.3",
    "@tamagui/metro-plugin": "^1.91.3",
    "@tamagui/shorthands": "^1.91.3",
    "@tamagui/themes": "^1.91.3",
    "@tamagui/toast": "^1.91.3",
    "airtable": "^0.12.2",
